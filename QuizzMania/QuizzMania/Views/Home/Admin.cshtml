@model IEnumerable<UsersModel> 
@{
    ViewData["Title"] = "Admin";
}

<h2>Admin</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">User</th>
            <th scope="col">Awnsers</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">1</th>
                <td>@item.FirstName</td>
                <td></td>
            </tr>
        }
    </tbody>
</table>


